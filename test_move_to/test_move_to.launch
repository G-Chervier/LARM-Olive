<launch>
    
    <arg name="world_file" value="$(find stage_ros)/world/willow-erratic.world" />
    
    <param name="/use_sim_time" value="true" />
	<include file="$(find tbot_bringup)/launch/minimal.launch"/>
    
    <node pkg="stage_ros" type="stageros" name="stage" args="$(arg world_file)"/>
    
    <node pkg="tf" type="static_transform_publisher" name="base_footprint_in_odom"
        args="2.3 4.0 0.01 0.8 0.0 0.0 /odom /base_footprint 80" />

    <node pkg="rviz" type="rviz" name="rviz" args="-d $(find turtlebot_stage)/rviz/robot_navigation.rviz"/>
    
    <node pkg="gmapping" type="slam_gmapping" name="gmapping_node" output="screen" >
		<remap from="scan" to="/base_scan" />
		<param name="odom_frame" value="/odom" />
	</node>
    <node pkg="test_move_to" type="move-to.py" name="gogogo" output="screen" />

</launch>
