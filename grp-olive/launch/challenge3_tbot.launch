<launch>

	<!-- <param name="/use_sim_time" value="true" /> -->
	<include file="$(find tbot_bringup)/launch/start.launch"/>
	<!-- Start Gmapping -->
		
	<node pkg="gmapping" type="slam_gmapping" name="gmapping_node" output="screen" >
		<param name="maxUrange" value="4.0"/> <!-- limit laser range to 4 meters -->
		<param name="odom_frame" value="/odom"/>
	</node>
	
	<!-- Start Rviz -->
	<node name="rviz" pkg="rviz" type="rviz" args="-d $(find grp-olive)/rviz/challenge3_tbot.rviz" />
	
	<node pkg="grp-olive" type="main.py" name="main" output="screen"></node>
	<node pkg="grp-olive" type="move.py" name="move" output="screen"></node>

			
</launch>
