<launch>

	<param name="/use_sim_time" value="true" />
	<include file="$(find tbot_bringup)/launch/minimal.launch"/>
	<!-- Start Gmapping -->
		
	<node pkg="gmapping" type="slam_gmapping" name="gmapping_node" output="screen" >
		<param name="maxUrange" value="4.0"/> <!-- limit laser range to 4 meters -->
		<param name="odom_frame" value="/odom"/>
	</node>

	<include file="$(find realsense2_camera)/launch/rs_camera.launch" >
		<param name="align_depth" value="true"/>
	</include>
	<!-- Start Rviz -->
	<node name="rviz" pkg="rviz" type="rviz" args="-d $(find grp-olive)/rviz/challenge2.rviz" />
	<node pkg="grp-olive" type="main.py" name="main" output="screen"></node>
	

</launch>
